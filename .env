# Enable local files serving for Label Studio
LOCAL_FILES_SERVING_ENABLED=true
LABEL_STUDIO_LOCAL_FILES_SERVING_ENABLED=true

# Set the document root to your datasets directory (use forward slashes)
LOCAL_FILES_DOCUMENT_ROOT=C:/data/media
LABEL_STUDIO_LOCAL_FILES_DOCUMENT_ROOT=C:/data/media

# Bind URL host for cookies and same-origin checks
HOST=http://localhost:8081